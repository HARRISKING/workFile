
.head{
  height: 180px;
  background-color: rgb(239, 239, 239);
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content:space-around;
}
.h4{
  color: rgb(33, 104, 164);
}
.box{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.box img{
  margin-bottom: 10px;
}
#boxAll{
  width: 92%;
  margin: 20px auto;
}
#boxAll .mark{
  background-color: rgb(33, 104, 164);
  width: 10px;
  height: 10px;
  border-radius: 50%;
  margin-right: 10px;
  margin-top: 3px;
  float:left;
}
#boxAll>div{
margin-bottom: 20px;
}
.inner-content{
}
.inner-content p:last-child{
  margin-top: 10px;
  line-height: 26px;
}
