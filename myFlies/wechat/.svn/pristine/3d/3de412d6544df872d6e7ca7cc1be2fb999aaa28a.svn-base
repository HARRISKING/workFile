div,ul,li,p,img,a{
  margin: 0;
  padding: 0;
  text-decoration: none;

}
a,button,input{
  -webkit-tap-highlight-color:rgba(255,0,0,0);
}
html{
  font-size: 17px;
}
body{
  margin:0;
  background: rgb(26, 51, 85);
}
p{
  font-family: "微软雅黑";
  color: #fff;
}
#content{
  /*border: 1px solid #ccc;*/
  margin: 0 auto;
}
#content-all{
  margin: 8% auto;
}

.header img{
  width: 6%;
}
.header p{
  color: #fff;
  position: relative;
  top: -3px;
  left: 5px;
  font-size: 0.8rem;
}
.header{
  margin: 10px 0;
}
#head-pic{
  margin:30px auto;
  margin-top: 0px;
}
#head-pic img{
  max-width: 210px;
  width: 40%;

  margin-left: 5%;
}

.title-big{
  margin-left: 5%;
}
.h1{
  font-size: 1.1rem;
  margin: 0.5rem 0rem 1rem 0rem;

}
.h2{
  font-size: 0.7rem;
  /*margin: 0.5rem 0rem 2rem 0rem;*/
}
#part{
  width: 90%;
  margin: 0 auto;
}
#part:after{
  content:"";
  display: block;
  clear: both;
}
.inner-text{
  display: inline-block;
}
.box-all{
  display: flex;
  flex-direction: row;
}
.box-left{
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
  width: 48%;
  height: 33rem;
}
.box1{
  text-align: center;
  margin-bottom: 3.55%;
  width: 70%;
  /*margin-left: 3%;*/

}
.box1 img{
  position: relative;
  top: 25px;
  /*left: -3px;*/
  height:30%;
}
.box1>a p{
  position: relative;
  top: 2.2rem;
  font-size: 0.7rem;
}
.box1 a{
  text-decoration: none;
  align-items: center;
}
.line{
  text-decoration: line-through;
  font-size: 0.7rem;
  color: #fff;
  position: relative;
  top:2rem;
}
.box2{
  width: 74%;
  height: 9rem;
  display: flex;
  flex-wrap: wrap;
  font-size: 0.7rem;
  align-items: center;
  /*justify-content: flex-end;*/
  /*flex-direction: column;*/
}
.box2 div{
  text-align: center;
  margin-left: 0.4rem;
  margin-bottom: 0.4rem;
  /*position: relative;*/
  display: -webkit-fix;
  display: flex;
  justify-content: center;
  flex-direction: column;

  /*默认宽度为两列宽度*/
  width:46%;
}


/*DIV区，用户将需要的样式直接放在该元素class中*/

/*左边大快：*/
.row1{
  height: 2.53rem;
}
.row2{
  height: 5.47rem;
}
.row3{
  height: 8.4rem;
}






/*行*/
.box2 .row-three{
  height: 2.53rem;
}
.box2 .row-two{
  height: 4rem;
}
.box2 .row-one{
  height: 8.4rem;
}

/*列*/
.box2 .column-one{
  width:95%;
}
.box2 .column-two{
  width:46%;
}
.box2 .column-three{
  width:29.67%;
}
.box2 .new-column{
  width:64%;
}

/*模板区：使用时将其放入父元素*/

.r3-c3 div{
  height: 2.53rem;
  width:29.67%;
}
.r3-c2 div{
  height: 2.53rem;
  width:46%;
}
.r3-c1 div{
  height: 2.53rem;
  width:95%;
}
.r2-c3 div{
  height: 4rem;
  width:29.67%;
}
.r2-c2 div{
  height: 4rem;
  width:46%;
}
.r2-c1 div{
  height: 4rem;
  width:95%;
}
.r1-c3 div{
  height: 8.4rem;
  width:29.67%;
}
.r1-c2 div{
  height: 8.4rem;
  width:46%;
}
.r1-c1 div{
  height: 8.4rem;
  width:95%;
}


/*颜色区：*/

.green1{
  background: linear-gradient(-45deg,rgb(39, 58, 87),rgb(46, 188, 157));
}
.green2{
  background: linear-gradient(45deg,rgb(46, 188, 157),rgb(39, 58, 87));
}
.purple{
  background: linear-gradient(-45deg,rgb(69, 57, 114),rgb(147, 101, 210));
}
.blue{
  background: linear-gradient(45deg,rgb(42, 54, 95),rgb(63, 158, 227));

}
.light-blue1{
  background: linear-gradient(-45deg,rgb(28, 79, 112),rgb(1, 183, 209));
}
.light-blue2{
  background: linear-gradient(135deg,rgb(28, 79, 112),rgb(1, 183, 209));
}

/*动画部分：*/

@keyframes overturn {
  0%{transform: rotateY(0deg);}
  24.99%{transform: rotateY(87deg);}
  50%{transform: rotateY(-10deg);}
  66%{transform: rotateY(10deg);}
  82%{transform: rotateY(-5deg);}
  99%{transform: rotateY(5deg);}
  100%{transform: rotateY(0deg);}
}
.animate{
  -webkit-animation: overturn 3s;
}



/*专配置给page-first*/
@media screen and (max-width: 350px){
  .box1 a .tittle{
    font-size: 0.9rem;
    font-weight: bold;
    margin: 0.8rem 0.5rem 0.5rem 0.5rem;
  }
}
@media screen and (min-width: 350px){
  .box1 a .tittle{
    font-size: 1rem;
    font-weight: bold;

    margin: 0.8rem 0.5rem 0.5rem 0.5rem;
  }
}
@media screen and (min-width: 1200px){
  #content{
    width: 1200px;
  }
}

.row-self{
  height: 30%;
}

.column-self{
  width:102%;
}

.r-c-self div{
  height: 8.4rem;
  width:95%;
}

.right-margin{
  margin-right: 3.55%;
}
