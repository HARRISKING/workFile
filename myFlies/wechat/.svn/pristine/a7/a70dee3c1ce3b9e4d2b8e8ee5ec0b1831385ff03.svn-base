.panel1{
  background-color: #fff;
  width: 100%;
  border-bottom: 2px solid #eee;
}

.box1{
  display: flex;
}

.box1>img{
  width: 80px;
  height: 80px;
  margin: 10px;
}

.box2{
  margin: 10px;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}

.box2 p{
  font-size: 13px;
  color: #999;
  line-height: 18px;
}

.box3{
  display: flex;
  justify-content: space-between;
}

.box3 img{
  width: 15px;
  height: 15px;

}
