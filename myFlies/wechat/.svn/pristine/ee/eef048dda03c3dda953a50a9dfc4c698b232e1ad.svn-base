div,ul,li,p,img{
  margin: 0;
  padding: 0;
}
html{
  font-size: 17px;
}
p{
  font-family: "微软雅黑";
  color: #fff;
}
#content{
  border: 1px solid #ccc;
  width: 100%;
  margin: 0 auto;
  background: rgb(26, 51, 85);
}
#content-all{
  margin: 8% auto;
}

.header img{
  width: 6%;
}
.header p{
  color: #fff;
  position: relative;
  top: -3px;
  left: 10px;
  font-size: 1rem;
}
.header{
  margin: 10px 0;
}
#head-pic{
  margin:30px auto;
  margin-top: 0px;
}
#head-pic img{
  width: 40%;
}
#part{
  width: 90%;
  margin: 0 auto;
}
.inner-text{
  display: inline-block;
}
#box-all:after{
  content:"";
  display: block;
  clear: both;
}
.box1{
  width: 31%;
  height: 8.4rem;
  float: left;
  text-align: center;
  background: rgb(48, 193, 160);
  position: relative;
  display: flex;
  display: -webkit-flex;
  flex-direction: column;
}
.box1 img{
  position: relative;
  top: 25px;
  left: -3px;
  width:60%;
}
.box1>a p{
  position: relative;
  top: 2.2rem;
  text-decoration: none;
  font-size: 0.7rem;
}
.box1 a{
  text-decoration: none;
  display: flex;
  display: -webkit-flex;
  flex-direction: column;
  align-items: center;
  flex-wrap: wrap;
}
.line{
  text-decoration: line-through;
  font-size: 0.7rem;
  color: #fff;
  position: relative;
  top:2rem;
}
.box2{
  width: 69%;
  float: right;
  display: -webkit-fix;
  display: flex;
  justify-content: center;
  flex-wrap: wrap;

}
.box-small{
  /*float: right;*/
  /*display: inline-block;*/
  width:46%;
  height: 4rem;
  text-align: center;
  /*line-height: 5rem;*/
  margin-left: 0.4rem;
  margin-bottom: 0.4rem;
  /*position: relative;*/
  display: -webkit-fix;
  display: flex;
  justify-content: center;
  align-items:center;
  flex-direction: column;


}
.box-small >p:last-child{
  /*position: absolute;
  top: 0.5rem;
  left: 1.7rem;*/
  font-size: 0.7rem;
  /*text-align: center;*/

}
.box-small >p:first-child{
  /*position: absolute;
  top: -1rem;
  left: 0.5rem;*/
  /*text-align: center;*/
  font-size: 0.7rem;

}
.b1{
  background: rgb(151, 105, 214);
}

.b2{
  background: rgb(48, 193, 160);
}
.b3{
  background: rgb(2, 188, 213);
}
.b4{
  background: rgb(65, 162, 231);
}
