div,ul,li,p{
  margin: 0;
  padding: 0;
  box-sizing: border-box;
  list-style: none;
}
html{
  font-size: 7px;
}
#box{
  max-width: 100%;
  margin: 0 auto;
  border: 1px solid #ccc;
  background: rgb(26, 51, 85);
}
#inner-box{
  max-width: 90%;
  margin: 8% auto;
}
#head{
  margin-bottom: 10%;
}
#head >img{
  width: 45%;
}
#part-all{
  margin-top: 5%;
  list-style: none;
  /*display: inline-block;*/
}
#part-all:after{
  content: "";
  display: block;
  clear: both;
}
.little-tittle{
  margin-bottom: 2%;
}
.cube{
  width: 6%;
}
.title{
  display: inline-block;
  color: #fff;
  font-family: "微软雅黑";
  font-size: 2.5rem;
  position: relative;
  left: 2%;
  top: -0.6rem;
}
.part p{
  font-family: "微软雅黑";
  font-size: 1.9rem;
}
.part{
  text-align: center;
  margin-left: 2%;
}
.part>a{
  text-decoration: none;
  color: #fff;
}

.part1-1{
  width: 35%;
  height: 16rem;
  float: left;
  padding: 5px;
  margin-left: 0px;
}
.part1-1>a{
  position: relative;
  top: 10px;
}

.part1-1 img{
  width: 50%;
}
.part1-2{
  width: 20%;
  height: 16rem;
  float: left;
}
.part1-2 img{
  width: 80%;
}
.p2-up a{
  position: relative;
  top: 2rem;
}
.part1-3{
  width:41%;
  height:7.55rem;
  float: right;
}
.part1-3 p{
  /*display: inline-block;*/

}
.part1-3 img{
  width: 17%;
  position: relative;
  top: 6px;
}
.part1-4{
  margin-top: 2%;
  float: right;
  width: 41%;
  height:7.55rem;
}
.part1-4 p{
  display: block;
  margin-top: 4px;
}
.part1-4 img{
  position: relative;
  top: 1rem;
  width: 20%;
}
.ohter1-1>a{
  position: relative;
  top: 1rem;
}
.ohter1-2>a{
  position: relative;
  top: 0.3rem;
}
.ohter1-2 p{
  margin-top: 0.3rem;          .
}
.active1-4 img{
  position: relative;
  top: 0.3rem;
}
.active1-4 p{
  display: block;
  margin-top: 10px;
}
.active-p4 img{
  position: relative;
  top: 1rem;
}
.active-p4 p{
  margin-top: 0.5rem;
}
.active-p2 img{
  width: 70%;
}
.active-p8 img{
  position: relative;
  top: 0.5rem;
}
.active-p8 p{
  display: block;
  margin-top: 2px;
}

.p4-down p{
  display: block;
}
.p4-down a{
  position: relative;
  top: -1rem;
}

.p1{
  background-color: rgb(48,193, 160);
}
.p2{
  background: rgb(65, 162, 231);
}
.p3{
  background: rgb(152, 104, 214);
}
.p4{
  background: rgb(2, 188, 213);
}
.p5{
  background: rgb(105, 54, 172);
}
.p6{
  background: rgb(2, 188, 213);
}
.p7{
  background: rgb(152, 104, 214);
}
.p8{
  background: rgb(65, 162, 231);
}
.p9{
  background: rgb(65, 162, 231);
}
.p10{
  background: rgb(152, 104, 214);
}
.p11{
  background: rgb(65, 162, 231);
}
.p12{
