img,ul,li,div,p,a{
  margin: 0;
  padding:0;
  list-style: none;
}
html{
  font-size: 17px;
  font-family: "微软雅黑"
}
#box{
  border: 1px solid #ccc;
  width: 100%;
  background: rgb(26, 51, 85);
  position: relative;
}
#box:after{
  content: "";
  display: block;
  clear: both;
}
.logo{
  width: 40%;
}
.inner-title{
  display: inline-block;
}
#box-content{
  width: 95%;
  margin: 8% auto;
}
#main-up{
  width: 100%;
  background:rgb(48, 193, 160);
}
.cube2{
  width: 4%;
  position: relative;;
  left: 3px;
  top: 3px;
}
.normal-wash{
  position: relative;
  top: 10px;
  left: -10px;
}
.word1{
  color: #fff;
  font-family: "微软雅黑";
  font-size: 0.8rem;
  margin-left: 10px;
  margin-top: 1rem;
  margin-bottom: 1rem;
}
.word2{
  color: #fff;
  font-family: "微软雅黑";
  font-size: 2rem;
  position: relative;
  top: -2rem;
  left: 1rem;
}
.inner-content{
  width:90%;
  margin: 0 auto;
  /*padding: 5% 0;*/
}
#main-down{
  text-align: center;
}
#main-down:after{
  content: "";
  display: block;
  clear: both;
}
.content-text p{
  color: #fff;
  font-size: 1rem;
  padding: 0.5rem  0rem 1rem  0rem;
}
.part{
  display: inline-block;
  width: 30%;
  height: 4.5rem;
  float: right;
  margin-right: 13px;
  margin-top: 18px;
  margin-bottom: 18px;
  background: rgb(37, 132, 124);
  color: #fff;
  /*display: flex;
  display: -webkit-flex;
  flex-direction: column;
  align-items: center;
  flex-wrap: wrap;*/
}

.part >a{
  text-decoration: none;
  color: #fff;
}
.part>a p{
  font-size: 17px;
}
.active-part>a p{
  margin-top: 0.5rem;
}
#main-down :first-of-type{
  margin-right: 1px;
}
#main-down{
  width:100%;
  margin: 0 auto;
}
#footer{
  height: 50px;
  text-align: center;
  line-height: 50px;
  background: rgb(37, 132, 124);
}
#footer>a{
  text-decoration: none;
  color: #fff;
}
